<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.thamer.myapplication.MainActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="1">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="60dp">


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@mipmap/ic_launcher"
                android:id="@+id/imageView"
                android:layout_weight="1" />
            <Button
                android:text="30"
                android:background="#01DF01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Tab30"
                android:layout_weight="1" />
            <Button
                android:text="31"
                android:background="#01DF01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Tab31"
                android:layout_weight="1" />
            <Button
                android:text="32"
                android:background="#01DF01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Tab32"
                android:layout_weight="1" />
            <Button
                android:text="33"
                android:background="#01DF01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Tab33"
                android:layout_weight="1" />
            <Button
                android:text="34"
                android:background="#01DF01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Tab34"
                android:layout_weight="1" />
            <Button
                android:text="35"
                android:background="#01DF01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Tab35"
                android:layout_weight="1" />
            <Button
                android:text="36"
                android:background="#01DF01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Tab36"
                android:layout_weight="1" />


        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="149dp"
            android:layout_weight="0.76">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <Button
                    android:text="50"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/but50"
                    android:background="#01DF01"
                    android:layout_weight="1" />

                <Button
                    android:text="56"
                    android:background="#01DF01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/but56"
                    android:layout_weight="1" />

                <Button
                    android:text="55"
                    android:background="#01DF01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/but55"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <Button
                        android:text="43"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#01DF01"
                        android:id="@+id/but43"
                        android:layout_weight="1" />

                    <Button
                        android:text="44"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/but44"
                        android:background="#01DF01"
                        android:layout_weight="1" />
                </LinearLayout>

                <Button
                    android:text="40"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#01DF01"
                    android:id="@+id/but40" />

                <Button
                android:text="41"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                    android:background="#01DF01"
                android:id="@+id/but41" />
                <Button
                    android:text="42"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#01DF01"
                    android:id="@+id/but42" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="55dp"
                    android:layout_height="match_parent">

                    <Button
                    android:text="20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/but20" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="60dp"
                    android:layout_height="match_parent">

                    <Button
                        android:text="21"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#01DF01"
                        android:id="@+id/but21" />

                    <Button
                        android:text="28"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#01DF01"
                        android:id="@+id/but28" />

                    <Button
                android:text="27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                        android:background="#01DF01"
                android:id="@+id/but27" />

                    <Button
                        android:text="26"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#01DF01"
                        android:id="@+id/but26" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:text="22"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#01DF01"
                        android:id="@+id/but22" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:background="#01DF01"
                        android:layout_height="79dp">

                        <Button
                            android:text="23"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#01DF01"
                            android:id="@+id/but23"
                            android:layout_weight="1" />

                        <Button
                    android:text="24"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                            android:background="#01DF01"
                    android:id="@+id/but24"
                            android:layout_weight="1" />
                    </LinearLayout>

                    <Button
                    android:text="25"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                        android:background="#01DF01"
                    android:id="@+id/but25" />
                </LinearLayout>


            </LinearLayout>


        </LinearLayout>


    </LinearLayout>

</LinearLayout>
